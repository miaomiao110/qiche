html {
	     color: #333;
	     font:normal 12px '微软雅黑','Microsoft YaHei';
}
body,ul,ol,p,h1,h2,h3 {
  margin: 0;
	        padding: 0;
        }
        ul,ol {
	        list-style: none;
        }
        a {
	        text-decoration: none;
	        color: #333;
        }
        .body{
        	overflow-x: hidden;
        	position: relative;
        }
        .go{
            line-height: 2em;
            font-size: 30px;
            opacity: 0.5;
        }
        .go a {
            display: block;
        }
        .go a {
            position: fixed;
            top: 15px;
            left: 15px;
            color: #fff;
        }
        .go>a>i{
           font-size: 30px;
        }
        .top{
        	width: 100%;
        	display: flex;
        	flex-direction: column;
        }
        .top>div:nth-child(1)>img{
        	width: 100%;
        	flex: 0 0 250px;
        	font-size: 0;
        }
        .top>div:nth-child(2){
        	height: 50px;
        	background-color: orange;
        	color: #fff;
        }
        .top>div:nth-child(2)::after{
        	display: block;
        	content: "";
        	clear: both;
        }
        .top>div:nth-child(2)>div{
            float: left;
        }
        .top>div:nth-child(2)>div:first-child{
        	width: 80%;
        	line-height: 50px;
        	padding-left: 20px;
        	font-size: 20px;
        }
        .top>div:nth-child(2)>div:last-child{
        	width: 20%
        	line-height:50px;
            font-size: 30px;
            font-weight: bold;
        }
        .top>div:nth-child(3){
        	height: 50px;
        	padding-left: 20px
        }
        .top>div:nth-child(3)>ul>li:nth-child(1){
        	line-height: 20px;
            font-size: 10px;
        }
        .top>div:nth-child(3)>ul>li:nth-child(2){
        	line-height: 30px;
        	font-size: 20px;
        }
        .top>div:nth-child(4){
        	line-height: 3em;
        	padding-left: 4em;
        	background-color: lightblue;
        }
        .car>div{
        	margin-top: 5px;
        	background-color: #ededed;
        }
        .car>div:nth-child(1)>ul>li:first-child{
        	float: left;
        	line-height: 3em;
        	width:70%;
        	padding-left: 2em;
        }
        .car>div:nth-child(1)>ul>li:last-child{
        	line-height: 3em;
        	width:30%;
        	padding-left: 2em;
        	margin-left: 70%;
        }
        .car>div:nth-child(2)::after{
        	display: block;
        	content: "";
        	clear: both;
        }
        .car>div:nth-child(2)>div{
        	float: left;
        	width: 33%;
        	height: 50px;
        	background-color:lightblue;
        	margin-right: 0.5%;
        }
        .car>div:nth-child(2)>div:nth-child(3),
        .car>div:nth-child(2)>div:nth-child(6){
            margin-right: 0;
        }
        .car>div:nth-child(2)>div>ul>li:first-child{
        	font-size: 10px;
        	line-height:20px;
        	padding-left: 1em;
        }
        .car>div:nth-child(2)>div>ul>li:last-child{
        	font-size: 18px;
        	font-weight: bold;
        	line-height: 30px;
        	padding-left: 1em;
        }
        .car>div:nth-child(3)::after{
        	display: block;
        	content: "";
        	clear: both;
        }
        .car>div:nth-child(3)>div{
        	float: left;
        	width: 20%;
        	height: 60px;
        	background-color:lightpink;
        }
        .car>div:nth-child(3)>div>ul>li{
            text-align: center;
        }
        .car>div:nth-child(3) i{
            font-size: 35px;
        }
        .car>div:nth-child(4){
        	display: line-block;
        	padding-left: 35%;
        	line-height: 3em;
        }
        .content>div:nth-child(1){
            width: 100%;
            line-height: 3em;
            padding-left: 1em;
            font-weight: bold;
            margin-bottom: 1px solid #333;
        }
        .content>div>ul>li{
        	line-height: 2em;
        	padding-left: 1em;
        	border-bottom: 1px solid #ededed;
        }
        .content>div>ul>li:hover>ul>li{
        	display: block;
        }
        .content>div>ul>li>ul>li{
        	text-align: center;
        	display: none;
        }
        .first{
        	display: flex;
        	flex-direction: column;
        	margin-top: 5px;
        	background-color: lightblue;
        }
        .first>ul>li{
        	border-bottom: 1px solid #ededed;
        }
        .first>ul>li:first-child{
        	text-align: center;
        	line-height: 2em;
        	font-weight: bold;
        }
        .first>ul>li:nth-child(3)>img{
        	width: 100%;
        	flex: 0 0 250px;
        	font-size: 0;
        }
        .first>ul>li:nth-child(4),
        .first>ul>li:nth-child(5),
        .first>ul>li:nth-child(6){
        	line-height: 2em;
        	padding-left: 2em;
        }
        .first>ul>li:hover>ul>li {
        	display: block;
        }
        .first>ul>li>ul>li{
        	text-align: center;
        	line-height: 3em;
        	background-color: pink;
        	display: none;
        }
        .second{
        	display: flex;
        	flex-direction: column;
        	margin-top: 5px;
        	background-color: lightpink;
        }
        .second>ul>li:first-child{
        	text-align: center;
        	line-height: 2em;
        	font-weight: bold;
        }
        .second>ul>li>ul>li:nth-child(1)>img{
        	width: 100%;
        	flex: 0 0 250px;
        	font-size: 0;
        }
        .second>ul>li>ul>li:nth-child(2){
        	padding-left: 2em;
        	line-height: 2em;
        }
        /*.bottom{
        	position: absolute;
        	bottom: 0px;*/
        }
        .bottom::after{
        	display: block;
        	content: "";
        	clear: both;
        }
        .bottom>ul>li{
        	float: left;
        }
        .bottom>ul>li:nth-child(1),
        .bottom>ul>li:nth-child(2){
        	width: 20%;
        	text-align: center;
        	
        }
        .bottom>ul>li:nth-child(3),
        .bottom>ul>li:nth-child(4){
        	width: 30%;
        	text-align: center;
        	line-height: 3em;
        	background-color: orange;
        	color: #fff;
        }