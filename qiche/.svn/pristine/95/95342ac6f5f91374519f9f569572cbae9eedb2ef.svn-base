		html,body,.content {
			height: 100%;
			width: 100%;
			margin: 0;
			padding: 0;
		}
		ul {
			margin: 0;
			padding: 0;
			list-style: none;
		}
		a {
			text-decoration: none;
			color: black;
		}
		.content {
			display: flex;
			flex-direction: column;
			background-color: teal;
		}

/*第一行  搜索*/
		.top {
			flex: 0 0 100px;
			background-color: lightblue;

			display: flex;
			flex-direction: column;
		}
		.search {
			flex: 1 1 50px;
			background-color: #fff;
			text-align: center;
		}
		.search>ul>li {
			float: left;
			width: 33%;
			line-height: 50px;
		}
		.search>ul::after {
			content: "";
			display: block;
			clear: both;
		}
		.search>ul>li:first-child {
			text-align: left;
			text-indent: 3em;
			font-size: 24px;
			font-weight: bold;
			font-style: italic;
		}
		.search input {
			width: 100%;
			height: 40px;
			border-radius: 25px;
			text-indent: 6em;
			border-style: none;
			background-color: #f6f6f6;
		}
		.search>ul>li:nth-child(2) {
			position: relative;
		}
		.search>ul>li:nth-child(3) {
			text-align: right;
			/*text-indent: 15em;*/

		}
		.icon-soushuo {
			position: absolute;
			left: 1.5em;
			top: 2px;
			font-size: 20px;
		}
		.icon-shijian {
			font-size: 30px;
		}

/*第二行  筛选*/
		.shaixuan {
			flex: 0 0 200px;
			background-color: #fdfdfd;

			display: flex;
			flex-direction: column;
		}
		.shaixuan_top {
            flex:0 0 50px;
            background-color: #fff;
			
			z-index: 2;
		}
		.shaixuan_content {
			flex: 1;
		}
		.shaixuan_top>ul>li {
			float: left;
            width: 20%;
            text-align: center;
            line-height: 50px;

            position: relative;
		}
		.shaixuan_top>ul>li:first-child {
			color: #e77535;
		}
		.shaixuan_top>ul::after {
			content: "";
			display: block;
			clear: both;
		}
		.shaixuan_top>ul>li:hover {
			color: #e6672b;
			background-color: #ededed;
		}
	
	/*智能筛选*/
		.aaa {
			position: absolute;
			z-index: 2;
			left: 0;
			top: 100%;
			
			background: #fff;
			width: 100%;
			color: #333;
			display: none;
		}
		.aaa>li {
			/*text-indent: 2em;*/
			text-align: center;
		}
		.shaixuan_top>ul>li:nth-child(1):hover>.aaa {
			display: block;
		}
		.shaixuan_top>ul>li:nth-child(1)>.aaa>li:hover {
			color: #e6672b;
			background-color: #fbefe6;
		}

	/*品牌*/
		.shaixuan_top>ul>li:nth-child(2):hover>.aaa {
			display: block;
		}
		.shaixuan_top>ul>li:nth-child(2)>.aaa>li:hover {
			color: #e6672b;
			background-color: #fbefe6;
		}

	/*价格*/
		.shaixuan_top>ul>li:nth-child(3):hover>.aaa {
			display: block;
		}
		.shaixuan_top>ul>li:nth-child(3)>.aaa>li:first-child {
			color: #e6672b;
			background-color: #fbefe6;
		}
		.shaixuan_top>ul>li:nth-child(3)>.aaa>li:hover {
			color: #e6672b;
			background-color: #fbefe6;
		}

	/*分期*/
		.bbb {
			position: absolute;
			/*width: 100%;
			height: 100%;*/
			left: -70px;
			right: -70px;
			top: 100%;
			background-color: #fff;
			z-index: 0;
		}
		.bbb>li>ul>li {
			float: left;
			width: 33%;
		}
		.bbb>ul>li>ul::after {
			clear: both;
			content: "";
			display: block;
		}
		.shaixuan_top>ul>li:nth-child(4)>.bbb {
			display: none;
		}
		.shaixuan_top>ul>li:nth-child(4):hover>.bbb {
			display: block;
		}
		.shaixuan_top>ul>li:nth-child(4)>.bbb>li>ul>li:hover {
			color: #e6672b;
			background-color: #fbefe6;
		}
		.shaixuan_top>ul>li:nth-child(4)>.bbb>li>ul>li/*:first-child*/ {
			color: #333;
			background-color: #fff;
		}


	/*层罩*/
		.cengzhao {
			position: fixed;
			left: 0;
			top: 0;
			width: 100%;
			height: 100%;
			background-color: black;
			opacity: 0.5;
			z-index: -1;
			display: none;
		}
		.shaixuan_top>ul:hover>.cengzhao /*&& .shaixuan_top>ul>li:hover*/ {
			display: block;

		}
		.shaixuan_top>ul .cengzhao:hover {
			display: none;
		}

/*第三行  图片*/
		.shaixuan_content>ul>li {
			float: left;
            width: 25%;
            text-align: center;
            line-height: 150px;   
		}
		.shaixuan_content>ul::after {
			content: "";
			display: block;
			clear: both;
		}
		.shaixuan_content>ul>li>a {
			display: inline-block;
			width: 150px;
			height: 100px;
			background-color: #ebebeb;
			border-radius: 10px;
			margin-top: 30px;
			position: relative;
		}
		.shaixuan_content>ul>li>a img {
			width: 100%;
			position: absolute;
			top: 0;
			left: 0;
		}

/*第四行   内容*/

		.carselect {
			flex: 0 0 100px;
			padding: 2em;

			display: flex;
			flex-direction: row;
			background-color: #fff	;
			overflow: auto;

		}

		.carselect:hover {
			background: #d9d8d8;
		}
		.tu_content a {
			display: block;
			width: 100%;
			height: 100%;
		}


		.carselect>.tu>a {
			display: inline-block;
			width: 100%;
			height: 100%;
		}
		.carselect>div {
			height: 100%;
			border-radius: 25px;
		}
		.carselect>.tu {
			flex: 0 0 200px;
			background-color: blue;
			margin-right: 20px;
			background-image: url("../../images/maiche/6.png");
			background-size: contain;
			background-repeat: no-repeat;
		}
		.carselect>.tu_content {
			box-sizing: border-box;
			flex: 6 6 300px;
			background-color: #fff;
			padding: 0;
			font-size: 18px;

		}
		.tu_content ul>li:nth-child(1) {
			font-size: 18px;
		}
		.tu_content ul>li:nth-child(2) {
			font-size: 8px;
			line-height: 1.8em;
			color: #9b9b9b;
		}
		.tu_content ul>li:nth-child(3) {
			font-size: 8px;
			background-color: #fbf1e0;
			color: #e5a044;
			width: 4em;
			border-radius: 14px;
			text-align: center;
		}
		.tu_content ul>li:nth-child(4) {
			color: #eb5728;
			font-size: 20px;
			line-height: 1.8em;
		}
		
		.gouwuche {
			position: relative;
		}
		.gouwuche .icon-gouwuche {
			position: absolute;
			right: 30px;
			top: -15px;
			font-size: 54px;
		}

/*最后一部分 底端nav*/
		.bottom {

			/*flex: 0 0 50px;*/

			position: fixed;
			height: 100px;
			background-color: #fff;
			bottom: 0;
			width: 100%;
			text-align: center;
			line-height: 40px;
			font-size: 18px;
			color: #333;
			font-weight: bold;
		}
		.bottom a {
			display: inline-block;
			width: 100%;
			height: 100%;
			background-color: #fff;

		}
		.bottom>ul>li {
			float: left;
			width: 20%;
		}
		.bottom>ul::after {
			clear: both;
			display: block;
			content: "";
		}
		.last_car {
			margin-bottom: 100px;
		}
		.icon-fangzi1,
		.icon-che,
		.icon-qian,
		.icon-gouwuche,
		.icon-yonghu {
			font-size: 32px;
		}
		.icon-che {
			color: #ec6f2d;
		}