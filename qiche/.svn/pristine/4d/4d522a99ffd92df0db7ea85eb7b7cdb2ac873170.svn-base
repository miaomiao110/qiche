html {
	font:normal 12px 'Microsoft YaHei',serif;
	width: 100%;
	height: 100%;
}
body{
	width: 100%;
	height: 100%;
}
body,p {
	margin: 0;
}
a {
	text-decoration: none;
}
ul {
	margin: 0;
	padding: 0;
	list-style: none;
}
/*上面*/
