}
	

		/*最底菜单栏*/

		.bottom {

			/*flex: 0 0 50px;*/

			position: fixed;
			height: 100px;
			background-color: #fff;
			bottom: 0;
			width: 100%;
			text-align: center;
			line-height: 40px;
			font-size: 18px;
			color: #333 !important;
			font-weight: bold;
		}
		.bottom a {
			display: inline-block;
			width: 100%;
			height: 100%;
			background-color: #fff;

		}
		.bottom>ul>li {
			float: left;
			width: 20%;
		}
		.bottom>ul::after {
			clear: both;
			display: block;
			content: "";
		}
		.last_car {
			margin-bottom: 100px;
		}
		.icon-fangzi1,
		.icon-che,
		.icon-qian,
		.icon-gouwuche,
		.icon-yonghu {
			font-size: 32px !important;
		}
		.icon-gouwuche {
			color: #ec6f2d;
		}