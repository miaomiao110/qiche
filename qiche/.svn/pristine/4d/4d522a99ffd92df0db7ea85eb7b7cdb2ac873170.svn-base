input{
	background-color: #ff6600;
	color: white;
	width: 10%;
	height: 50%;
	position: absolute;
	margin-left: 75%;
	margin-right: 0;
	margin-top: 1.5%;
	margin-bottom: 1.5%;
	border-bottom-right-radius: 10px;
	border-bottom-left-radius: 10px;
	border-top-left-radius: 10px;    
	border-top-right-radius: 10px;
	border-style: none;
}
			/*底部*/

.gwcnr{
width: 100%;
height: 70%;
background-color: #f9f9f9;
position: absolute;
}
.gwcnrbg{
	width: 50%;
	height: 80%;
	position: relative;
	margin-top: 5%;
	margin-bottom: 5%;
